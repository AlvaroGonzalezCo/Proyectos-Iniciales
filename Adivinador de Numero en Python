import random

numero = random.randint(1,20)
contador = 0
print ("Adivina el numero entre 1 y 20: ")


while contador < 6:
  guess = int(input())

  if guess == numero:
    print("SI PAPU")
    break
  
  elif guess < numero:
    print("Es mas grande bro")

  elif guess > numero:
   print("Es mas chico bro") 

  contador += 1

if contador == 6:
  print("Perdiste, el numero era", numero) 
   


 

